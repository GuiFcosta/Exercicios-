#include<stdio.h>
int matriz(int mat1[][3], int N)
{
    int i, j;

    printf("Matriz:\n");

    for(i=0; i<N; i++)
    {
        for(j=0; j<3; j++)
        {
            printf("%d", mat1[i][j]);
        }
        printf("\n");
    }
}
int main()
{
    int L;

    printf("Digite o numero de linhas da matriz: ");
    scanf("%d",&L);

    int mat[L][3];
    matriz(mat,L);
}
